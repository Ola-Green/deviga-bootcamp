{
  "name": "bootcamp-server",
  "version": "1.0.0",
  "description": "mongo express node",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "client":"cd client && npm run start",
    "server-install":"npm install",
    "client-install":"cd client && npm install",
    "install-all": "concurrently \"npm run server-install\" \"npm run client-install\" ",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "Ola-Green"
  },
  "author": "Ola Green",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cloudinary": "^1.25.1",
    "concurrently": "^6.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "googleapis": "^72.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.5",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.5.0",
    "path": "^0.12.7",
    "peer": "^0.6.1",
    "socket.io": "^4.1.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
